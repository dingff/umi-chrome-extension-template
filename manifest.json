{
  "action": {
    "default_icon": {
      "128": "./logo/logo.png",
      "16": "./logo/logo.png",
      "32": "./logo/logo.png",
      "48": "./logo/logo.png"
    },
    "default_popup": "./popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["./content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "description": "{{插件的描述}}",
  "icons": {
    "128": "./logo/logo.png",
    "16": "./logo/logo.png",
    "32": "./logo/logo.png",
    "48": "./logo/logo.png"
  },
  "manifest_version": 3,
  "name": "{{插件名称}}",
  "permissions": ["storage", "tabs"],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "0.0.1"
}
